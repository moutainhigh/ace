<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.wxiaoqi.security.schedulewo.mapper.BizFlowDispatchRuleMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.github.wxiaoqi.security.schedulewo.entity.BizFlowDispatchRule" id="bizFlowDispatchRuleMap">
        <result property="id" column="ID"/>
        <result property="flowId" column="flow_id"/>
        <result property="crontab" column="crontab"/>
        <result property="dispatchTime" column="dispatch_time"/>
        <result property="minutes" column="minutes"/>
        <result property="status" column="status"/>
        <result property="timeStamp" column="time_Stamp"/>
        <result property="createBy" column="create_By"/>
        <result property="createTime" column="create_Time"/>
        <result property="modifyBy" column="modify_By"/>
        <result property="modifyTime" column="modify_Time"/>
    </resultMap>

    <!-- 查询流程的调度规则  -->
    <select id="getDispatchRuleByFlowId" resultType="com.github.wxiaoqi.security.schedulewo.vo.BusDispatchRule">
            SELECT crontab crontab,dispatch_time dispatchTime,minutes minutes
            from biz_flow_dispatch_rule
            where flow_id=#{flowId}
            and status='1'
    </select>

</mapper>